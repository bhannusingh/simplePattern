import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		Scanner scanner = new Scanner(System.in);
		System.out.print("enter no. of star you want in a row");
		int row =scanner.nextInt();
		System.out.print("enter the no. of star you want in a col.");
		 int col =scanner.nextInt();
		
		for( int i=0;i<=row;i++){
		    for(int j=0;j<=col;i++){
		        System.out.print("*");
		    }
		System.out.println("");
		    
		}
	}
}
