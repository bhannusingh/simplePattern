import java.util.*;
public class Main{
    
    public static  void printPattern(int n)
    {
    int sum =1;
    int i;
    int j;
    for(i=1; i<=n;i++){
        for(j=1; j<=i;j++){
            System.out.print(" "+sum);
            sum++;
        }
        System.out.println();
    }
    }
    public static void main(String[]arg){
        int n= 10;
    printPattern(n);
    }
}
