import java.util.*;
public class Main{
public static void printPattern(int n){
    int i,j;
    int sum=0;
    for(i =1; i<=n;i++){
        for(j=1;j<=n-i;j++){
            System.out.print(sum+" ");
            sum++;
        }
        System.out.println();
    }
}
public static void main (String[]args){
    int n=6;
    printPattern(n);
}
}
